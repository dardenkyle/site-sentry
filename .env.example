# Site Sentry Configuration
# Copy this file to .env and customize as needed

# Target website URL
BASE_URL=https://kyledarden.com

# Test configuration
HEADLESS=true
SLOWMO=0
TIMEOUT=30000

# Browser configuration
BROWSER=chromium

# Playwright options
VIEWPORT_WIDTH=1280
VIEWPORT_HEIGHT=720

# Test output
TEST_RESULTS_DIR=test-results
SCREENSHOTS_DIR=test-results/screenshots

# Logging
LOG_LEVEL=INFO
